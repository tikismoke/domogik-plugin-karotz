    "xPL_wakeup": {
      "name": "wakeup via curl",
      "schema": "exec.basic",
      "xplstat_name": "xPL_ack-msg",
      "parameters": {
        "static": [],
        "device": []
      }
    },
    "xPL_sleep": {
      "name": "Sleep via curl",
      "schema": "exec.basic",
      "xplstat_name": "xPL_ack-msg",
      "parameters": {
        "static": [],
        "device": []
      }
    }
