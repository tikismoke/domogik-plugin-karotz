        {
          "key": "body",
          "data_type": "DT_String",
          "conversion": ""
        }
      ],
      "xpl_command": "xPL_send_msg"
    },
    "wakeup": {
      "name": "Wake up",
      "return_confirmation": true,
      "parameters": [
        {
          "key": "wakeup",
          "data_type": "DT_Trigger",
          "conversion": "from_DT_Trigger_to_start"
        }
      ],
      "xpl_command": "xPL_wakeup"
    },
    "sleep": {
      "name": "Sleep",
      "return_confirmation": true,
      "parameters": [
        {
          "key": "sleep",
          "data_type": "DT_Trigger",
          "conversion": "from_DT_Trigger_to_start"
        }
      ],
      "xpl_command": "xPL_sleep"
    }
